#!/bin/bash

function start()
{

	if [ -z $SERVER_HOME ]; then
	    echo "This script can't be invoked standalone! Exiting..."
	    exit 1
	fi

	echo "Starting $SERVER_NAME..."

    exec java -cp "$CONF:$LIB/*:$SERVER_HOME/job.jar" $JAVA_OPTIONS $SERVER_MAIN_CLASS $CONF/dogs.properties >>${SERVER_LOG_DIR}/${NAME}-server${SERVER_LOG_SUFIX}.out 2>&1
	echo "$SERVER_NAME started"
}

start
